        "prepare": "npm run build && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UpperCod/css-to-object.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/UpperCod/css-to-object/issues"
    },
    "homepage": "https://github.com/UpperCod/css-to-object#readme",
    "dependencies": {},
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^8.1.0",
        "ava": "^3.10.1",
        "builtin-modules": "^3.1.0",
        "esm": "^3.2.25",
        "rollup": "^2.21.0"
    },
    "typings": "types.d.ts",
    "ava": {
        "require": [
            "esm"
        ],
        "files": [
            "test/**/*.test.js"
        ]
